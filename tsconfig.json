{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "outDir": "es5",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "newLine": "LF"
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}
